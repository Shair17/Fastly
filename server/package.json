{
	"name": "fastly-server",
	"private": true,
	"version": "1.0.0",
	"scripts": {
		"start": "yarn build && node scripts/clear && node dist/main",
		"dev": "yarn env:dev && node scripts/clear && nodemon | pino-pretty -c",
		"env:dev": "cross-env NODE_ENV=development",
		"build": "yarn clean && tsc",
		"clean": "rimraf dist",
		"format": "prettier --write \"src/**/*.ts\"",
		"createSecret": "node scripts/createSecret"
	},
	"license": "MIT",
	"dependencies": {
		"@fastify/compress": "^5.0.0",
		"@fastify/cors": "^7.0.0",
		"@fastify/env": "^3.0.0",
		"@fastify/helmet": "^8.0.1",
		"@fastify/rate-limit": "^6.0.0",
		"@fastify/routes": "^4.0.0",
		"@fastify/routes-stats": "^3.0.0",
		"@fastify/static": "^5.0.2",
		"ajv": "^8.6.1",
		"argon2": "^0.28.5",
		"axios": "^0.27.2",
		"class-validator": "^0.13.2",
		"cross-env": "^7.0.3",
		"fastify": "^3.29.0",
		"fastify-decorators": "^3.11.0",
		"fastify-favicon": "^3.1.0",
		"fastify-healthcheck": "^3.1.0",
		"fastify-multer": "^2.0.2",
		"http-errors": "^2.0.0",
		"http-status-codes": "^2.2.0",
		"jsonwebtoken": "^8.5.1",
		"mysql2": "^2.3.3",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.3.6"
	},
	"devDependencies": {
		"@types/http-errors": "^1.8.1",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/node": "^16.0.0",
		"nodemon": "^2.0.14",
		"pino-pretty": "^7.2.0",
		"prettier": "^2.4.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.4.0",
		"typescript": "^4.4.4"
	}
}
